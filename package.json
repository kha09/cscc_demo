{
  "name": "csccproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "migrate:dev": "dotenv -e scripts/.env.development -- npx prisma migrate dev --schema=prisma/schema.prisma",
    "migrate:reset": "dotenv -e scripts/.env.development -- npx prisma migrate reset --schema=prisma/schema.prisma",
    "generate": "dotenv -e scripts/.env.development -- npx prisma generate --schema=prisma/schema.prisma",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node prisma/seed.js",
    "seed:controls": "dotenv -e scripts/.env.development -- node dist/scripts/importControls.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "apexcharts": "^4.6.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "csv-parse": "^5.6.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.476.0",
    "next": "14.2.16",
    "next-auth": "^4.24.11",
    "prisma": "^6.5.0",
    "react": "^18",
    "react-apexcharts": "^1.7.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "postcss": "^8",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
